{
  "name": "municollect-monorepo",
  "version": "0.2.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "cd apps/frontend && npm run build",
    "dev": "cd apps/frontend && npm run dev",
    "lint": "cd apps/frontend && npm run lint",
    "typecheck": "cd apps/frontend && npm run typecheck",
    "frontend:dev": "cd apps/frontend && npm run dev",
    "backend:dev": "cd apps/backend && go run cmd/server/main.go"
  },

  "devDependencies": {
    "typescript": "^5"
  },
  "packageManager": "npm@10.0.0"
}
